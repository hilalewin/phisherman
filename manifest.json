{
    "name": "Chrome Extension MV3",
    "version": "0.1.0",
    "manifest_version": 3,
    "icons": {
        "48": "icon2.png"
    },
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_icon": {
            "48": "icon2.png"
           
        },
        "default_title": "Click to Sign in with Google"

    },
    "permissions": [
        "storage",
        "activeTab",
        "scripting",
        "identity",
        "notifications",
        "tabs"
    ],
    "host_permissions": [
        "https://mail.google.com/*",
        "https://www.googleapis.com/*",
        "https://*.googleusercontent.com/*"
    ],
    "oauth2": {
        "client_id": "765430000956-j83fv4lh7fc18mb1cn283adrfkrlc3ga.apps.googleusercontent.com",
        "scopes": [
            "profile",
            "https://www.googleapis.com/auth/gmail.readonly"
        ]
    },
    "key": ""
}